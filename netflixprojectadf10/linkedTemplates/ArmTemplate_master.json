{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "netflixprojectadf10"
		},
		"datalake_input_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalake_input_ls'"
		},
		"github_input_http_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'github_input_http'"
		},
		"ls_databricks_notebook_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_databricks_notebook'"
		},
		"datalake_input_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://netflixprojectsa10.dfs.core.windows.net/"
		},
		"github_input_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com"
		},
		"github_input_http_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "anupama"
		},
		"ls_databricks_notebook_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0326-105332-etd5f9nd"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "netflixprojectadf10_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datalake_input_ls_accountKey": {
						"value": "[parameters('datalake_input_ls_accountKey')]"
					},
					"github_input_http_password": {
						"value": "[parameters('github_input_http_password')]"
					},
					"ls_databricks_notebook_accessToken": {
						"value": "[parameters('ls_databricks_notebook_accessToken')]"
					},
					"datalake_input_ls_properties_typeProperties_url": {
						"value": "[parameters('datalake_input_ls_properties_typeProperties_url')]"
					},
					"github_input_http_properties_typeProperties_url": {
						"value": "[parameters('github_input_http_properties_typeProperties_url')]"
					},
					"github_input_http_properties_typeProperties_userName": {
						"value": "[parameters('github_input_http_properties_typeProperties_userName')]"
					},
					"ls_databricks_notebook_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_notebook_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}