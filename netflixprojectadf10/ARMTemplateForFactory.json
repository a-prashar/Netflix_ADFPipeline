					"quoteChar": "\""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/github_input_http')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ds_lookup_file_gen2')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "datalake_input_ls",
					"type": "LinkedServiceReference"
				},
